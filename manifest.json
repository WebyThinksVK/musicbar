{
  "background": {
    "page": "background.html"
  },
  "content_scripts": [
    {
      "css": [
        "styles/style.css"
      ],
      "js": [
        "main.js"
      ],
      "matches": [
        "http://vk.com/*",
        "http://*.vk.com/*",
        "https://vk.com/*",
        "https://*.vk.com/*"
      ],
      "run_at": "document_idle"
    }
  ],
  "description": "Дополнительный функционал аудиозаписей ВКонтакте. ",
  "icons": {
    "128": "icons/icon_128.png",
    "16": "icons/icon_16.png",
    "48": "icons/icon_48.png",
    "64": "icons/icon_64.png"
  },

  "browser_action": {
    "default_icon": "icons/icon_48.png"
  },
  "key": "",
  "manifest_version": 2,
  "minimum_chrome_version": "18",
  "name": "Music Bar",
  "permissions": [
    "storage",
    "webRequest",
    "webRequestBlocking",
    "downloads",
    "<all_urls>"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "5.2.7",
  "externally_connectable": {
    "matches": [
      "http://vk.com/*",
      "http://*.vk.com/*",
      "https://vk.com/*",
      "https://*.vk.com/*",
      "http://*.lastfm.ru/*"
    ]
  },
  "web_accessible_resources": [
    "styles/style.css",
    "modals/form_equalizer.html",
    "modals/performer.html",
    "modals/select.html",
    "scripts/newAudioPlayer.js",
    "scripts/VoiceMessagePlayer.js",
    "scripts/speech_settings.js",
    "panel.html"
  ]
}


